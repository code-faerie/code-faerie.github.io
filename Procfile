tailwind-watch: npx @tailwindcss/cli -i ./src/style.css -o ./assets/style.css --watch
tailwind-build: npx @tailwindcss/cli -i ./src/style.css -o ./assets/style.css
jekyll-watch: bundle exec jekyll serve --host 0.0.0.0 --watch
jekyll-build: bundle exec jekyll build
jekyll-serve: bundle exec jekyll serve --host 0.0.0.0
